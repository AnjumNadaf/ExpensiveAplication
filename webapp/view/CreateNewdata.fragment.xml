<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sf="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified">
	<Dialog  title="Employee Register">
		<VBox class="sapUiSmallMargin">
		<sf:SimpleForm editable="true" width="100%" layout="ResponsiveGridLayout">
		<sf:content>
				<Label text=""></Label>
				<Input id="input01" placeholder="Enter Emp Id" liveChange="changeId"></Input>
				<Label text=""></Label>
				<Input id="input02" type="Text" placeholder="Enter Emp Name" liveChange="changeName"></Input>
				<Label text=""></Label>
				<Input id="input03"  placeholder="Enter Emp LastName" type="Text" liveChange="changeLastName"></Input>
				<Label text=""></Label>
				<Input id="input04"  placeholder="Enter Designation" type="Text" liveChange="changeDesignation"></Input>
				<Label text=""></Label>
				<Input id="input05"  placeholder="Enter Password" type="Password" liveChange="changePassowrd"></Input>
				<Label text=""></Label>
				<Input id="input06"  placeholder="Enter Email" type="Email" liveChange="changeEmail"></Input>
				<Label text=""></Label>
				<Input id="input07"  placeholder="Enter Adress" type="Text" liveChange="changeAdress"></Input>
				<Label text=""></Label>
				<Input id="input08"  placeholder="Enter Age" type="Number"   maxLength="2" liveChange="changeAge"></Input>
				<Label text=""></Label>
				<Input id="input09"  placeholder="Enter Contact No" type="Number" maxLength="10"  liveChange="changeContactNo"></Input>
				
				</sf:content>
			</sf:SimpleForm>
		
		
		
		</VBox>
			
		
		<buttons>
            <Button text="Submit" type="Accept" press="onSubmit"></Button>
			<Button text="Close" press="onClose" type="Reject"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>